[["aka_name", "cast_info", "company_name", "movie_companies", "name", "role_type", "title"], {"a1": "aka_name", "ci": "cast_info", "cn": "company_name", "mc": "movie_companies", "n1": "name", "rt": "role_type", "t": "title"}, [{"names": ["cn"], "condition": "cn.country_code = '[us]'"}, {"names": ["rt"], "condition": "rt.role = 'writer'"}, {"names": ["a1", "n1"], "condition": "a1.person_id = n1.id"}, {"names": ["ci", "n1"], "condition": "n1.id = ci.person_id"}, {"names": ["ci", "t"], "condition": "ci.movie_id = t.id"}, {"names": ["mc", "t"], "condition": "t.id = mc.movie_id"}, {"names": ["cn", "mc"], "condition": "mc.company_id = cn.id"}, {"names": ["ci", "rt"], "condition": "ci.role_id = rt.id"}, {"names": ["a1", "ci"], "condition": "a1.person_id = ci.person_id"}, {"names": ["ci", "mc"], "condition": "ci.movie_id = mc.movie_id"}], "SELECT MIN(a1.name) AS writer_pseudo_name, MIN(t.title) AS movie_title FROM aka_name AS a1, cast_info AS ci, company_name AS cn, movie_companies AS mc, name AS n1, role_type AS rt, title AS t WHERE cn.country_code ='[us]' AND rt.role ='writer' AND a1.person_id = n1.id AND n1.id = ci.person_id AND ci.movie_id = t.id AND t.id = mc.movie_id AND mc.company_id = cn.id AND ci.role_id = rt.id AND a1.person_id = ci.person_id AND ci.movie_id = mc.movie_id;", [[[2, 3], {"join_type": "Merge Join"}], [[4, 0], {"join_type": "Merge Join"}], [[6, 1], {"join_type": "Merge Join"}], [[7, 9], {"join_type": "Merge Join"}], [[10, 8], {"join_type": "Merge Join"}], [[11, 5], {"join_type": "Merge Join"}]]]